<?xml version="1.0"?>
<config>
    <modules>
        <Aitoc_Aitconfcheckout>
            <version>2.1.11</version>
        </Aitoc_Aitconfcheckout>
    </modules>
    <frontend>
        <routers>
            <aitconfcheckout>
                <use>standard</use>
                <args>
                    <module>Aitoc_Aitconfcheckout</module>
                    <frontName>aitconfcheckout</frontName>
                </args>
            </aitconfcheckout>
        </routers>
        <events/>
        <translate>
            <modules>
                <Aitoc_Aitconfcheckout>
                    <files>
                        <default>Aitoc_Aitconfcheckout.csv</default>
                    </files>
                </Aitoc_Aitconfcheckout>
            </modules>
        </translate>
        <layout>
            <updates>
                <aitconfcheckout>
                    <file>aitconfcheckout.xml</file>
                </aitconfcheckout>
            </updates>
        </layout>
    </frontend>
    <default>
        <router>aitconfcheckout</router>
        <config_vars/>
    </default>
    <global>
        <rewrite>
            <aitoc_aitconfcheckout_onepage>
                <from>#^/checkout/onepage/#</from>
                <to>/aitconfcheckout/onepage/</to>
            </aitoc_aitconfcheckout_onepage>
        </rewrite>
        <models>
            <aitconfcheckout>
                <class>Aitoc_Aitconfcheckout_Model</class>
                <resourceModel>aitconfcheckout_mysql4</resourceModel>
            </aitconfcheckout>
            <checkout>
                <rewrite>
                    <type_onepage>Aitoc_Aitconfcheckout_Model_Rewrite_FrontCheckoutTypeOnepage</type_onepage>
                </rewrite>
            </checkout>
            <sales>
                <rewrite>
                    <service_quote>Aitoc_Aitconfcheckout_Model_Rewrite_FrontSalesServiceQuote</service_quote>
                    <quote_address>Aitoc_Aitconfcheckout_Model_Rewrite_FrontSalesQuoteAddress</quote_address>
                </rewrite>
            </sales>
        </models>
        <events>
            <customer_address_save_after>
                <observers>
                    <aitpermissions>
                        <type>singleton</type>
                        <class>aitconfcheckout/observer</class>
                        <method>customerAddressSaveAfter</method>
                    </aitpermissions>
                </observers>
            </customer_address_save_after>
            <sales_quote_payment_save_before>
                <observers>
                    <aitpermissions>
                        <type>singleton</type>
                        <class>aitconfcheckout/observer</class>
                        <method>salesQuotePaymentSaveBefore</method>
                    </aitpermissions>
                </observers>
            </sales_quote_payment_save_before>
            <controller_action_predispatch_paypal_standard_redirect>
                <observers>
                    <aitpermissions>
                        <type>singleton</type>
                        <class>aitconfcheckout/observer</class>
                        <method>predispatchPaypalStandardRedirect</method>
                    </aitpermissions>
                </observers>
            </controller_action_predispatch_paypal_standard_redirect>
            <paypal_prepare_line_items>
                <observers>
                    <aitpermissions>
                        <type>singleton</type>
                        <class>aitconfcheckout/observer</class>
                        <method>paypalPrepareLineItems</method>
                    </aitpermissions>
                </observers>
            </paypal_prepare_line_items>
            <controller_action_predispatch_paypal_express_review>
                <observers>
                    <aitpermissions>
                        <type>singleton</type>
                        <class>aitconfcheckout/observer</class>
                        <method>predispatchPaypalExpressReview</method>
                    </aitpermissions>
                </observers>
            </controller_action_predispatch_paypal_express_review>
            <sales_quote_address_collection_load_after>
                <observers>
                    <aitpermissions>
                        <type>singleton</type>
                        <class>aitconfcheckout/observer</class>
                        <method>quoteAddressCollectionLoadAfter</method>
                    </aitpermissions>
                </observers>
            </sales_quote_address_collection_load_after>
            <adminhtml_controller_action_predispatch_start>
                <observers>
                    <aitconfcheckout>
                        <type>model</type>
                        <class>aitconfcheckout/checkout_cart_observer</class>
                        <method>onAdminhtmlControllerActionPredispatchStart</method>
                    </aitconfcheckout>
                </observers>
            </adminhtml_controller_action_predispatch_start>
            <core_locale_set_locale>
                <observers>
                    <aitconfcheckout>
                        <type>model</type>
                        <class>aitconfcheckout/customer_observer</class>
                        <method>onCoreLocaleSetLocale</method>
                    </aitconfcheckout>
                </observers>
            </core_locale_set_locale>
        </events>
        <resources>
            <aitconfcheckout_setup>
                <setup>
                    <module>Aitoc_Aitconfcheckout</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </aitconfcheckout_setup>
            <aitconfcheckout_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </aitconfcheckout_write>
            <aitconfcheckout_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </aitconfcheckout_read>
        </resources>
        <blocks>
            <aitconfcheckout>
                <class>Aitoc_Aitconfcheckout_Block</class>
            </aitconfcheckout>
            <checkout>
                <rewrite>
                    <cart_shipping>Aitoc_Aitconfcheckout_Block_Rewrite_FrontCheckoutCartShipping</cart_shipping>
                </rewrite>
                <rewrite>
                    <onepage>Aitoc_Aitconfcheckout_Block_Rewrite_FrontCheckoutOnepage</onepage>
                </rewrite>
                <rewrite>
                    <onepage_progress>Aitoc_Aitconfcheckout_Block_Rewrite_FrontCheckoutOnepageProgress</onepage_progress>
                </rewrite>
                <rewrite>
                    <onepage_billing>Aitoc_Aitconfcheckout_Block_Rewrite_FrontCheckoutOnepageBilling</onepage_billing>
                </rewrite>
                <rewrite>
                    <onepage_shipping>Aitoc_Aitconfcheckout_Block_Rewrite_FrontCheckoutOnepageShipping</onepage_shipping>
                </rewrite>
            </checkout>
        </blocks>
        <helpers>
            <aitconfcheckout>
                <class>Aitoc_Aitconfcheckout_Helper</class>
            </aitconfcheckout>
        </helpers>
    </global>
    <default>
        <aitconfcheckout>
            <billing>
                <company>1</company>
                <address>1</address>
                <city>1</city>
                <country>1</country>
                <region>1</region>
                <postcode>1</postcode>
                <telephone>1</telephone>
                <fax>1</fax>
            </billing>
            <shipping>
                <active>1</active>
                <company>1</company>
                <address>1</address>
                <city>1</city>
                <country>1</country>
                <region>1</region>
                <postcode>1</postcode>
                <telephone>1</telephone>
                <fax>1</fax>
            </shipping>
            <shipping_method>
                <active>1</active>
            </shipping_method>
            <payment>
                <active>1</active>
            </payment>
        </aitconfcheckout>
    </default>
    <adminhtml>
        <translate>
            <modules>
                <Aitoc_Aitconfcheckout>
                    <files>
                        <default>Aitoc_Aitconfcheckout.csv</default>
                    </files>
                </Aitoc_Aitconfcheckout>
            </modules>
        </translate>
    </adminhtml>
</config>
